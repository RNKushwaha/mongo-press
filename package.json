{
    "name": "mongo-press",
    "version": "1.0.0",
    "description": "Mongo-press API",
    "private": true,
    "scripts": {
        "build": "npx tsc",
        "createbuild": "yarn build | npm run build",
        "prestart": "yarn createbuild && pm2 delete all || : && pm2 flush",
        "start": "pm2 start ecosystem.config.js --env development",
        "start:prod": "pm2 start ecosystem.prod.config.js --env production",
        "dev": "node -r dotenv/config dist/index.js dotenv_config_path=.env dotenv_config_debug=true",
        "prepare": "husky install"
    },
    "keywords": [
        "API"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm test"
        }
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown",
        "**/*.ts": "eslint ./",
        "*.js": "eslint --cache --fix",
        "*.{js,css,md}": "prettier --write"
    },
    "author": "RN Kushwaha",
    "license": "MIT",
    "_moduleAliases": {
        "@utils": "dist/utils",
        "@middlewares": "dist/middlewares"
    },
    "dependencies": {
        "body-parser": "^1.20.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "express": "^4.18.1",
        "helmet": "^6.0.0",
        "module-alias": "^2.2.2",
        "morgan": "^1.10.0",
        "winston": "^3.8.2",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "@types/compression": "^1.7.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/morgan": "^1.9.3",
        "@types/node": "^18.7.14",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "dotenv": "^16.0.2",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard-with-typescript": "^23.0.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^15.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "git-format-staged": "^3.0.0",
        "husky": "^8.0.0",
        "lint-staged": "^13.0.3",
        "prettier": "2.7.1",
        "pretty-quick": "^3.1.3",
        "tsconfig-paths": "^4.1.0",
        "typescript": "*"
    },
    "main": "index.js",
    "repository": "https://github.com/RNKushwaha/mongo-press.git"
}
